        <property>
          <name>profile</name>
          <value>dev</value>
        </property>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <skip.assembly>true</skip.assembly>
        <skip.checkstyle>false</skip.checkstyle>
