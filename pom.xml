      <!-- Run JUnit tests during "test" phase -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>${surefire.plugin.version}</version>
        <configuration>
          <!-- Sets the VM argument line used when unit tests are run. -->
          <argLine>${surefireArgLine}</argLine>
          <skipTests>${skipTests}</skipTests>
          <excludes>
            <exclude>**/TestDb*.java</exclude>
            <exclude>**/TestMeasurementGarbageCollector.java</exclude>
          </excludes>
        </configuration>
      </plugin>
      <!-- Run QA tools during "test" phase -->
      <!-- Findbugs -->
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>findbugs-maven-plugin</artifactId>
        <version>${findbugs.plugin.version}</version>
        <configuration>
          <excludeFilterFile>${findbugs.filter.file}</excludeFilterFile>
          <skip>${skip.findbugs}</skip>
        </configuration>
        <executions>
          <execution>
            <phase>test</phase>
            <goals>
              <goal>check</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <!-- Checkstyle -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-checkstyle-plugin</artifactId>
        <version>${checkstyle.plugin.version}</version>
        <configuration>
          <configLocation>${checkstyle.filter.file}</configLocation>
          <skip>${skip.checkstyle}</skip>
        </configuration>
        <executions>
          <execution>
            <phase>test</phase>
            <goals>
              <goal>check</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <!-- PMD -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-pmd-plugin</artifactId>
        <version>${pmd.plugin.version}</version>
        <configuration>
          <targetJdk>${targetJdk}</targetJdk>
          <linkXref>false</linkXref>
          <sourceEncoding>${project.build.sourceEncoding}</sourceEncoding>
          <minimumTokens>100</minimumTokens>
          <skip>${skip.pmd}</skip>
        </configuration>
        <executions>
          <execution>
            <phase>test</phase>
            <goals>
              <goal>check</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <!-- JavaDoc -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>${javadoc.plugin.version}</version>
        <configuration>
          <linksource>true</linksource>
          <!-- uncomment following line for Java8 -->
          <!-- <additionalparam>-Xdoclint:none</additionalparam> -->
        </configuration>
        <executions>
          <execution>
            <phase>test</phase>
            <goals>
              <goal>javadoc</goal>
              <goal>test-javadoc</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <!-- Run QA & Reporting tools during "site" phase -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-site-plugin</artifactId>
        <version>${site.plugin.version}</version>
        <configuration>
          <reportPlugins>
            <plugin>
              <groupId>org.codehaus.mojo</groupId>
              <artifactId>findbugs-maven-plugin</artifactId>
              <version>${findbugs.plugin.version}</version>
              <configuration>
                <excludeFilterFile>${findbugs.filter.file}</excludeFilterFile>
              </configuration>
            </plugin>
            <plugin>
              <groupId>org.apache.maven.plugins</groupId>
              <artifactId>maven-checkstyle-plugin</artifactId>
              <version>${checkstyle.plugin.version}</version>
              <configuration>
                <configLocation>${checkstyle.filter.file}</configLocation>
              </configuration>
            </plugin>
            <plugin>
              <groupId>org.apache.maven.plugins</groupId>
              <artifactId>maven-pmd-plugin</artifactId>
              <version>${pmd.plugin.version}</version>
              <reportSets>
                <reportSet>
                  <reports>
                    <report>pmd</report>
                    <report>cpd</report>
                  </reports>
                </reportSet>
              </reportSets>
              <configuration>
                <targetJdk>${targetJdk}</targetJdk>
                <linkXref>false</linkXref>
                <sourceEncoding>${project.build.sourceEncoding}</sourceEncoding>
                <minimumTokens>100</minimumTokens>
              </configuration>
            </plugin>
            <plugin>
              <groupId>org.apache.maven.plugins</groupId>
              <artifactId>maven-javadoc-plugin</artifactId>
              <version>${javadoc.plugin.version}</version>
            </plugin>
            <!--<plugin>-->
              <!--<groupId>org.jacoco</groupId>-->
              <!--<artifactId>jacoco-maven-plugin</artifactId>-->
              <!--<version>0.6.3.201306030806</version>-->
