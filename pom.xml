          <value>rel</value>
        </property>
      </activation>
      <properties>
        <skip.assembly>false</skip.assembly>
        <skip.checkstyle>false</skip.checkstyle>
        <skip.findbugs>false</skip.findbugs>
        <skip.pmd>false</skip.pmd>
        <skipTests>false</skipTests>
